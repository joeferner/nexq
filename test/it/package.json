    "type": "git"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "radash": "^12.1.0",
    "typia": "^8.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "swagger-typescript-api": "^12.0.4",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.0"
